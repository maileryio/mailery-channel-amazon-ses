{
    "name": "maileryio/mailery-channel-email-aws",
    "type": "library",
    "description": "AWS Email channel module for Mailery Platform",
    "keywords": [
        "mail",
        "mailer",
        "mailinig",
        "aws",
        "ses"
    ],
    "homepage": "https://github.com/maileryio/mailery-channel-email-aws",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/maileryio/mailery-channel-email-aws",
        "issues": "https://github.com/maileryio/mailery-channel-email-aws/issues",
        "wiki": "https://github.com/maileryio/mailery-channel-email-aws/wiki"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4",
        "foxy/foxy": "^1.0",
        "maileryio/mailery-channel-email": "dev-master",
        "yiisoft/form": "^1.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phan/phan": "^2.4",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "Mailery\\Channel\\Email\\Aws\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mailery\\Channel\\Email\\Aws\\Tests\\": "tests"
        }
    },
    "scripts": {
        "lint": "php-cs-fixer fix --diff --dry-run",
        "fix": "php-cs-fixer fix",
        "phan": "phan --progress-bar -o analysis.txt"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "config-plugin": {
            "params": "config/params.php",
            "common": "config/common.php",
            "web": "config/web.php",
            "providers-web": "config/providers-web.php"
        }
    },
    "config": {
        "sort-packages": true
    }
}
